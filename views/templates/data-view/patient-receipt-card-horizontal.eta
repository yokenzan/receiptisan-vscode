    <div class="section-horizontal-pane">
      <table class="patient-info-table">
          <tr>
            <th>ÊÇ£ËÄÖÁï™Âè∑</th>
            <td>
              <% if (it.patient.patientId === '-') { %><span class="placeholder">-</span><% } else { %><%= it.patient.patientId %><% } %>
            </td>
          </tr>
          <tr>
            <th>Ê∞èÂêç</th>
